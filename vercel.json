{
  "version": 2,
  "builds": [
    {
      "src": "web-server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "web-server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}
